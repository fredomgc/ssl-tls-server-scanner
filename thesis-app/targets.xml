<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<profiles>
		<profile name="low">			
			<protocols>
				<protocol name="SSLv2"  mode="mustBe"  />
				<protocol name="SSLv3"  mode="mustNotBe"  />
				<protocol name="TLS 1.0"  mode="mustBe"  />
				<protocol name="TLS 1.1"  mode="mayBe"  />
				<protocol name="TLS 1.2"  mode="mustNotBe" />
			</protocols>
			
			<vulnerabilitiesFree mode="mayBe" />
			<certificateValid  mode="mayBe">
				<directive name="rsaMinimumPublicKeySize" value="20"  mode="mustBe" />
				<directive name="rsaMinimumSignatureKeySize" value="20"  mode="mustBe" />
				<directive name="ecdsaMinimumPublicKeySize" value="20"  mode="mustBe"  />
				<directive name="ecdsaMinimumSignatureKeySize" value="20"  mode="mustBe"  />
			</certificateValid>
			
			<ciphers>
				<cipher name="ECDHE-ECDSA-DES-CBC3-SHA"  mode="mustNotBe"  />
			</ciphers>
		</profile>
	</profiles>
	
	<targets>
		<target destination="https://127.0.0.1/"  profile="low"  name="mÅ¯j localhost" />
	</targets>
</configuration>